{
    "name": "coenschutte/cronly-wrapper",
    "description": "This is my package cronly-wrapper",
    "keywords": [
        "CoenSchutte",
        "cronly-wrapper"
    ],
    "homepage": "https://github.com/coenschutte/cronly-wrapper",
    "license": "MIT",
    "authors": [
        {
            "name": "Coen Schutte",
            "email": "me@coenschutte.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "CoenSchutte\\CronlyWrapper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CoenSchutte\\CronlyWrapper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
